{
  "expo": {
    "name": "UBICompShowcase",
    "slug": "UBICompShowcase",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.jo3115.UBICompShowcase",
      "versionCode": 1
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "description": ""
  },
  "scheme": "UBICompShowcase",
  "plugins": [
    [
      "async-storage",
      {
        "AsyncStorage_db_size_in_MB": 20
      }
    ]
  ]
}
